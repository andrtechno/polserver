use os;
use uo;
use file;


program textcmd_addnpc(who, text)
    SendSysMessage(who, "You here: "+who.x+" "+who.y+" "+who.z);
    var str := "";
    str:=str+"Npc X\n";
    str:=str+"{\n";
    str:=str+"	Name				"+text+"\n";
    str:=str+"	X					"+who.x+"\n";
    str:=str+"	Y					"+who.y+"\n";
    str:=str+"	Z					"+who.z+"\n";
    str:=str+"	Realm				britannia\n";
    str:=str+"}\n";
    var l := LogToFile("::log/addnpc.log", str);
    if(!l)
        syslog("l: "+l.texterror);
    endif



endprogram