use uo;
use os;

program teleto(who)
  SendSysMessage(who, "Move Who?");
  sleepMS(100);
  var what := Target(who);
  if(!what)
    return;
  endif

  SendSysMessage(who, "Move where?");
  sleepMS(100);
  var where := TargetCoordinates(who);
  if(!where)
    return;
  endif
  MoveObjectToLocation(what, where.x, where.y, where.z, where.realm, MOVEOBJECT_FORCELOCATION);
endprogram