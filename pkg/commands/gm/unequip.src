use uo;

program textcmd_unbequip( who )
    SendSysMessage( who, "What shall be unequipped?", color := 88 );

    //var what := Target(who);
    var what := TargetCoordinates(who);

    if (!what)
        return;
    endif
    if(what.item.isa(POLCLASS_ITEM))
        MoveObjectToLocation(what.item, who.x, who.y, who.z, who.realm, MOVEOBJECT_FORCELOCATION);
    endif

 endprogram