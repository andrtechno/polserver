use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndStrengthPotion(params)
	var mobile := params[1];
	
	var points := GetObjProperty(mobile, "StrengthPotion");

	AP_ModifyStatMod(mobile, STRENGTH, -points.str);
	EraseObjProperty(mobile, "StrengthPotion");
	
	return 1;
endprogram

