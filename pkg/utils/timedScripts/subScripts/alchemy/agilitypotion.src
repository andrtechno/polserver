use uo;
use os;

include ":attributes:attributes";

program TimedScript_AgilityPotion(params)
	var mobile := params[1];
	var point := params[3];
	params := 0;

    var points := struct;
    points.+dex := CInt(point);
	SetObjProperty(mobile, "AgilityPotion", points);
	AP_ModifyStatMod(mobile, DEXTERITY, points.dex);
	
	return 1;
endprogram

