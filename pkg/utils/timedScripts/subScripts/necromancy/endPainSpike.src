use uo;
use os;

include ":attributes:attributes";
include ":timedScripts:timedScripts";

program TimedScript_EndPainSpike(params)
	var mobile := params[1];

	var cfg_elem := TS_GetCfgElem("PainSpike");
    var buff_icon := CInt( cfg_elem.BuffIcon );
	var points := struct;
	points := GetObjProperty(mobile, "PainSpike");
	mobile.delbuff(buff_icon);
	EraseObjProperty(mobile, "PainSpike");

	return 1;
endprogram

