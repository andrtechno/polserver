use uo;
use os;

include ":attributes:attributes";
include ":timedScripts:timedScripts";

program TimedScript_EndMortalStrike(params)
	var mobile := params[1];
	
	var cfg_elem := TS_GetCfgElem("MortalStrike");
    var buff_icon := CInt( cfg_elem.BuffIcon );

	mobile.delbuff(buff_icon);
	EraseObjProperty(mobile, "MortalStrike");

	return 1;
endprogram

