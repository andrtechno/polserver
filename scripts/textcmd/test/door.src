use uo;
use os;

program finddoor(who)

    var doorsList := {
    // metal door
    0x0675, 0x0677, 0x0679, 0x067b, 0x067d, 0x067f, 0x0680, 0x0681, 0x0683,
    // barred metal door
    0x0685, 0x0686, 0x0687, 0x0688, 0x0689, 0x068A, 0x068b, 0x068c, 0x068d, 0x068e, 0x068f, 0x0690, 0x0691, 0x0692, 0x0693,
    // rattan door
    0x0695, 0x0696, 0x0697, 0x0698, 0x0699, 0x069A, 0x069b, 0x069c, 0x069d, 0x069e, 0x069f, 0x06A0, 0x06A1, 0x06A2, 0x06A3,
    //wooden door
    0x06A5, 0x06a6, 0x06a7, 0x06a8, 0x06a9, 0x06AA, 0x06Ab, 0x06Ac, 0x06Ad, 0x06Ae, 0x06Af, 0x06B0, 0x06B1, 0x06B2, 0x06B3


    };
    foreach doorType in doorsList
        foreach item in ListItemsInBoxOfObjType(doorType, 0, 0, 0, 5131,4080,0, who.realm)
          if(!GetObjProperty(item,"signserial"))
            SendSysMessage(who, " "+item.x+", "+item.y);
            MoveObjectToLocation(who, item.x, item.y, item.z, who.realm, MOVEOBJECT_FORCELOCATION);
            sleepms(300);
          endif
        endforeach
    endforeach




endprogram
