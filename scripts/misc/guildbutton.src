use uo;
use os;

program core_guildButton( mobile )

	var guild := mobile.guildid;
	if( !guild )
		//Start_Script( ":guilds:guildCreate", mobile );
		SendSysMessage(mobile, "no guild");
		return 1;
	elseif( guild )
		//Start_Script( ":guilds:guildMenu", mobile );
		SendSysMessage(mobile, "already guild");
		return 1;
	endif

	return 1;
endprogram
