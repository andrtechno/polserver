use uo;
use os;
use unicode;

program cmd_shutdown()

    print("Saving World State");
    BroadcastUC("Сохранение игрового мира...", "RUS", 3, 0x25);
    var res := SaveWorldState();
    if (res)
        print("Done saving: " + res );
        BroadcastUC("Сохранение завершено.", "RUS", 3, 0x25);
    else
        print("Save error: " + res);
    endif

endprogram